{{define "specPage"}}
    {{$overview := (toOverview .SuiteRes .SpecRes.FileName)}}
    <!doctype html>
    <html>
      <head>
        <meta http-equiv="X-UA-Compatible" content="IE=9; IE=8; IE=7; IE=EDGE" />
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>Gauge Test Results</title>
        <link rel="shortcut icon" type="image/x-icon" href="{{(toPath $overview.BasePath "images/favicon.ico")}}" />
        <link rel="stylesheet" type="text/css" href="{{(toPath $overview.BasePath "css/vendor.css")}}" />
        <link rel="stylesheet" type="text/css" href="{{(toPath $overview.BasePath "css/app.css")}}" />
      </head>
      <body>
        {{template "header" $overview}}
        {{template "spec" .SpecRes}}
        {{template "sidebar" (toSidebar .SuiteRes .SpecRes.FileName)}}
        {{template "footer"}}
        <script language="javascript" src="{{(toPath $overview.BasePath "js/vendor.js")}}"></script>
        <script language="javascript" src="{{(toPath $overview.BasePath "js/main.js")}}"></script>
      </body>
    </html>
{{end}}